    Mod4 + XF86AudioRaiseVolume

"mpc volume -4"
    m:0x0 + c:122
    XF86AudioLowerVolume

"mpc volume -14"
    m:0x40 + c:122
    Mod4 + XF86AudioLowerVolume

"mpc toggle"
    m:0x0 + c:172
    XF86AudioPlay

"mpc stop"
    m:0x0 + c:174
    XF86AudioStop

"mpc seek +0:0:39"
